form setup
{
    layout = LAYOUT_ABSOLUTE
    batchingEnabled = false
    autoWidth = true
    autoHeight = true

    label title
    {
        position = 5%, 3%
        text = Wait for Start
        textColor = #60A010FF
        fontSize = 96
        font = res/ui/big.gpb
    }

    label chatLabel
    {
        position = 5%, 20%
        width = 10%
        height = 8%
        font = res/ui/big.gpb
        fontSize = 32
        text = Chat:
    }

    textBox chatText
    {
        position = 15%, 20%
        width = 65%
        height = 8%
        font = res/ui/little.gpb
        fontSize = 32
    }

    button chatButton
    {
        position = 85%, 20%
        width = 10%
        height = 8%
        font = res/ui/big.gpb
        fontSize = 24
        text = Send
    }

    button kickButton
    {
        enabled = false
        position = 85%, 70%
        width = 10%
        height = 8%
        text = Kick
        font = res/ui/big.gpb
        fontSize = 24
    }

    container playerListContainer
    {
        style = Form
        layout = LAYOUT_VERTICAL
        width = 65%
        height = 40%
        position = 15%, 40%
    }

    button backButton
    {
        position = 4%, 90%
        size = 10%, 8%
        text = < Back
        fontSize = 24
        font = res/ui/big.gpb
    }

    button startButton
    {
        position = 86%, 90%
        size = 10%, 8%
        text = Start >
        fontSize = 24
        font = res/ui/big.gpb
        enabled = false
    }
}

